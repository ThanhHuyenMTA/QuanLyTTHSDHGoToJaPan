<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" href="~/Content/images/logoNew.jpg" type="image/x-icon" />
    <!-- Bootstrap -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <!-- NProgress -->
    <link href="~/Content/css/nprogress.css" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="~/Content/css/gentelella-custom.min.css" rel="stylesheet" />
    <link href="~/Content/css/custom.css" rel="Stylesheet" type="text/css" />
    <!-- Table -->
    <link href="~/Content/table/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/table/css/buttons.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/table/css/fixedHeader.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/table/css/responsive.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/table/css/scroller.bootstrap.min.css" rel="stylesheet" />

    <script type="text/javascript" src="../../Content/js/jquery.min.js"></script>
    <script type="text/javascript" src="../../Content/js/bootstrap.min.js"></script>
    @* format datetime *@
    <script src="~/Scripts/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/bootstrap-datepicker.min.js"></script>
    @* END *@
    <title>@ViewBag.Title</title>
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            @Html.Action("Submenu", "Shares")
            <div class="right_col" role="main">
                @RenderBody()
            </div>
            @Html.Action("Footer", "Shares")
        </div>
    </div>

    <script type="text/javascript" src="../../Content/js/nprogress.js"></script>
    <script type="text/javascript" src="../../Content/js/gentelella-custom.min.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script type="text/javascript" src="../../Content/js/moment.min.js"></script>
    <script type="text/javascript" src="../../Content/js/daterangepicker.js"></script>
    <!-- Table -->
    <script src="~/Content/table/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/table/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Content/table/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/table/js/buttons.bootstrap.min.js"></script>
    <script src="~/Content/table/js/buttons.flash.min.js"></script>
    <script src="~/Content/table/js/buttons.html5.min.js"></script>
    <script src="~/Content/table/js/buttons.print.min.js"></script>
    <script src="~/Content/table/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/Content/table/js/dataTables.keyTable.min.js"></script>
    <script src="~/Content/table/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/table/js/responsive.bootstrap.js"></script>
    <script src="~/Content/table/js/dataTables.scroller.min.js"></script>
</body>
</html>
<style>
    .right_col{
        background-image:url("~/Content/images/banner.png");
    }
</style>